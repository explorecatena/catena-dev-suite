{
  "name": "catena-dev-suite",
  "version": "0.0.1",
  "description": "Catena Development Suite",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "NODE_ENV=development nodemon -w static -w views -w index.js index.js",
    "build": "npm-run-all build:*",
    "build:ganache": "cd lib/ganache && npm install && npm run compile:web && cd ../..",
    "build:remix": "cd lib/remix-ide && npm install && npm run build && cd ../.."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/bitaccess/catena-dev-suite.git"
  },
  "keywords": [
    "catena",
    "ethereum",
    "development",
    "suite"
  ],
  "author": "Dylan <dylan@bitaccess.co>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/bitaccess/catena-dev-suite#readme",
  "dependencies": {
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.17.4",
    "nunjucks": "^3.1.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "nodemon": "^1.17.1",
    "npm-run-all": "^4.1.2"
  }
}
